@using TravelTripProject.Models.Classes
@model List<Blog>
<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@item.BlogDescription<span>...</span></p>
        </div>
        <div class="clear"></div>
    }


</div>